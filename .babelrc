{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "> 1%"
      }
    }]
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": true,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    "transform-object-assign",
    "syntax-dynamic-import",
    "react-hot-loader/babel",
    "react-loadable/babel"
  ]
}
